  "peerDependencies": {
    "symbol-observable": ">=1.0.1"
  },
  "dependencies": {
    "babel-cli": "~6.9.0",
    "babel-plugin-jsx-pragmatic": "~1.0.2",
    "babel-plugin-syntax-jsx": "~6.8.0",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-stage-0": "~6.5.0",
