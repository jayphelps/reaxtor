{
  "name": "reaxtor",
  "version": "1.8.0",
  "description": "Falcor + Rx + virtual-dom",
  "main": "dist/index.js",
  "browser": "dist/index.js",
  "scripts": {
    "test": "node test/index.js",
    "clean": "rimraf dist && mkdirp dist",
    "build": "npm run clean && babel src -d dist --source-maps",
    "watch": "npm run clean && babel src -d dist --source-maps --watch",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trxcllnt/reaxtor.git"
  },
  "keywords": [
    "Falcor",
    "Reactive",
    "Extensions",
    "virtual-dom",
    "rx",
    "rxjs"
  ],
  "author": "Paul Taylor <paul.e.taylor@me.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trxcllnt/reaxtor/issues"
  },
  "homepage": "https://github.com/trxcllnt/reaxtor#readme",
  "peerDependencies": {
    "symbol-observable": ">=1.0.1"
  },
  "dependencies": {
    "babel-cli": "6.9.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "debug": "~2.2.0",
    "falcor": "trxcllnt/falcor#onChangesCompleted",
    "falcor-json-graph": "trxcllnt/falcor-json-graph",
    "falcor-path-syntax": "trxcllnt/falcor-path-syntax",
    "falcor-path-utils": "trxcllnt/falcor-path-utils",
    "falcor-router": "trxcllnt/falcor-router#rx-5-and-complex-refs",
    "left-pad": "~1.1.1",
    "mkdirp": "0.5.1",
    "rimraf": "2.5.2",
    "rxjs": ">=5.0.0-beta.10",
    "snabbdom-jsx": "0.3.0"
  }
}
